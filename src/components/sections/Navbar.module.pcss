.navbarContainer {
    display: inline-block;
    padding: 15px 20px;
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.menuElements {
    display: flex;
    & ul {
        list-style-type: none;
        & li {
            color: #fff;
            display: inline-block;
            margin-right: 15px;
            text-transform: uppercase;
        }
    }
}
.wallet-adapter-button-trigger {
    display: none;
}
.buttons{
    display:flex;
    gap: 10px;
}